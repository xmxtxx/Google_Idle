{
  "name": "Idle Resete",
  "description": "an Idle resete plugin for scs intern only",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.scs.ch/*"],
      "css": ["styles.css"]
    }
  ],
  "permissions": [
    "tabs",
    "idle"
  ]
}